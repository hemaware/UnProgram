import java.util.Scanner;

public class FirstProgram
{
		public static void main(String[] args)
		{
			System.out.println("Whats up out there?");
			System.out.println("Said computer will compute a solution with two inputs from User");
			System.out.println("Correct, that is a positive");
		
			int n1, n2;

			Scanner keyboardInput = new Scanner(Scanner.in);
			n1 = keyboardInput.nextInt();
			n2 = keyboardInput.nextInt();

			System.out.print("Your arithmatical solution comprising of ");
			System.out.print(n1);
			System.out.println(" and ");
			System.out.println(n2);
			System.out.println("is");
			System.out.println(n1 + n2);
			
			
		}
}
